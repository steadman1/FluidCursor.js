{
  "name": "fluidcursor",
  "version": "1.0.0",
  "description": "front-end TypeScript library for dynamic cursor interactions aiming to create more immersive 2D and 3D experiences with native Three.js support.",
  "main": "./dist/cjs/FluidCursor.js",
  "module": "./dist/esm/FluidCursor.js",
  "scripts": {
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build:esm": "tsc --project tsconfig.esm.json",
    "build": "npm run build:cjs && npm run build:esm"
  },
  "types": "./dist/cjs/FluidCursor.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/FluidCursor.js",
      "require": "./dist/cjs/FluidCursor.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/steadman1/FluidCursor.js.git"
  },
  "keywords": [
    "front-end",
    "typescript",
    "UI/UX",
    "cursor",
    "fluidcursor",
    "custom"
  ],
  "author": "spencer steadman",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/steadman1/FluidCursor.js/issues"
  },
  "homepage": "https://github.com/steadman1/FluidCursor.js#readme",
  "dependencies": {
    "gsap": "^3.12.5",
    "undici": "^6.19.8"
  },
  "devDependencies": {
    "@types/node": "20.0.0"
  }
}
